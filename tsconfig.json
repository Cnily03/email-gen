{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["DOM", "ESNext"],
    "allowSyntheticDefaultImports": true,
    "plugins": [{
      "name": "@astrojs/ts-plugin"
    }],
    "baseUrl": ".",
    "types": ["./global.d.ts"],
    "paths": {
      "@/*": ["src/*"],
      "@docs/*": ["./docs/*"],
    }
  },
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
